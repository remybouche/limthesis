file = {:home/rsmarinho/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Khanpour et al. - 2008 - A Wideband W-Band Receiver Front-End in 65-nm CMOS.pdf:pdf},
isbn = {0018-9200},
issn = {00189200},
journal = {IEEE Journal of Solid-State Circuits},
keywords = {Gilbert-cell mixer,Low-noise amplifiers (LNAs),Millimeter-wave imaging,Nanoscale CMOS,Noise in circuits with feedback,W-band},
month = {aug},
number = {8},
pages = {1717--1730},
title = {{A wideband W-band receiver front-end in 65-nm CMOS}},
