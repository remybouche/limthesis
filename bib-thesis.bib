isbn = {1553-877X},
issn = {1553877X},
journal = {IEEE Communications Surveys and Tutorials},
keywords = {5G,C-RAN,D2D,HetNets,IDMA,IoT,M2M,QoE,SDMA,SDN,SON,beamforming,channel model,field trials,massive MIMO,mm-wave,sustainability},
number = {3},
