journal = {IEEE Journal of Solid-State Circuits},
keywords = {60 GHz,GaAs,Highly integrated,MMIC,Multi-functional,RX,Receiver,Single-chip,TX,V-band,image rejection,mHEMT,transmitter},
month = {nov},
